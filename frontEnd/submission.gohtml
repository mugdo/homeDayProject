                </div>

                <div class="row">
                    <label for="submit-share" class="col-md-2 col-form-label">Share:</label>
                    <div class="col-md-10">
                        <div class="btn-group" data-toggle="buttons" id="submit-share">
                            <label class="btn btn-secondary">
                                <input type="radio" name="share" id="share1" value="1" autocomplete="off">Yes
                            </label>
                            <label class="btn btn-secondary active">
                                <input type="radio" name="share" id="share0" value="0" autocomplete="off">No
                            </label>
                        </div>
                    </div>
                </div>

                <div class="row">
                    <label for="submit-solution" class="col-md-2 col-form-label">Solution:</label>
                    <div class="col-md-10">
                        <textarea class="form-control" name="source" id="submit-solution" rows="15" placeholder="At least 50 characters"></textarea>
                    </div>
                </div>

                <div class="row captcha" style="display: none">
                    <label class="col-md-2 col-form-label">Captcha:</label>
                    <div class="col-md-10">
                        <input type="text" class="form-control" name="captcha">
                        <img src="#" id="submit-captcha-img">
                    </div>
                </div>
            </div>
        </form>
        </div>
    </div>
</div>

{{ template "close" }}