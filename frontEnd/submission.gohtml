                    <option value="HihoCoder">HihoCoder</option>
                    <option value="HIT">HIT</option>
                    <option value="HRBUST">HRBUST</option>
                    <option value="HUST">HUST</option>
                    <option value="HYSBZ">HYSBZ</option>
                    <option value="Kattis">Kattis</option>
